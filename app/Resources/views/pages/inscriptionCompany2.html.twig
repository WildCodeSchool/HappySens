{% extends 'base.html.twig' %}

{% block title %}Inscriptions{% endblock %}

{% block content %}

    <div id="contentInscription" class="parallax-container-others">
        <div class="section no-pad-bot">
            <div class="col s12">
                <div class="headerTitleOthers container" id="headerTitle">
                    <h1 class="header amber-text text-darken-2">InscriptionCompany</h1>
                    <div class="row">
                        <h5 class="header white-text">Libérez les passions et donnez du sens au travail</h5>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container">
        <form action="" method="post" class="generalContainer">
            <div class="row">
                <div class="col s12">
                    <h5 class="titleInscription pink-text text-darken-2">Slogan : Le bonheur au travail c'est pour nous :</h5>
                </div>
                <div class="input-field col s12">
                    <input id="city" name="city" type="text" class="validate">
                    <label for="city"><span class="requireInput">* </span>A compléter...</label>
                </div>
                <div class="col s12">
                    <h5 class="titleInscription pink-text text-darken-2">Les enjeux collectifs de la Qualité de Vie au Travail :</h5>
                </div>
                <div class="input-field col s12">
                    <input id="city" name="city" type="text" class="validate">
                    <label for="city"><span class="requireInput">* </span>A compléter...</label>
                </div>
                <div class="col s12">
                    <h5 class="titleInscription pink-text text-darken-2">Les 3 critères à remplir pour un Happy projet validé :</h5>
                </div>
                <div class="input-field col s12">
                    <input id="city" name="city" type="text" class="validate">
                    <label for="city"><span class="requireInput">* </span>A compléter</label>
                </div>
                <div class="col s12">
                    <p><span class="requireInput">* </span> Champs requis</p>
                </div>
            </div>
            <div class="row">
                <div class="input-field right">
                    <a href="{{ path('profilCompany') }}" class="btn waves-effect waves-light amber darken-2">
                        Valider
                        <i class="material-icons right">send</i>
                    </a>
                </div>
            </div>
        </form>
    </div>
{% endblock %}
